{
  "name": "toronto-blog",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.5",
    "bootstrap": "^4.3.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "husky": "^2.4.1",
    "jquery": "^3.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "mongoose": "^5.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "popper.js": "^1.15.0",
    "react": "^16.8.6",
    "react-cookie": "^3.1.2",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0-alpha.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.0",
    "redux": "^4.0.1",
    "redux-starter-kit": "^0.4.3",
    "secure-random": "^1.1.1",
    "toronto-utils": "./packages/toronto-utils"
  },
  "scripts": {
    "start": "npm run build-server && node dist/server.js",
    "build": "react-scripts build",
    "build-server": "babel src/server -d dist",
    "build-utils": "cd packages/toronto-utils && npm run build",
    "update-utils": "npm run build-utils && yarn upgrade ./packages/toronto-utils",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "heroku-postbuild": "npm run build",
    "start-dev-react": "npm run update-utils && react-scripts start",
    "start-dev-server": "nodemon --inspect=9230 --exec babel-node src/server/server.js"
  },
  "proxy": "http://localhost:5000/",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "rm -r ./packages/toronto-utils/lib/ && npm run build-utils"
    }
  }
}
